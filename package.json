{
  "name": "musiq",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "scp -r build/* musiq@musiq.dergunov.net:~/web",
    "android": "react-native run-android",
    "android:log": "react-native log-android",
    "android:release": "react-native run-android --variant release",
    "build:android:apk": "cd android && gradlew assembleRelease",
    "build:android:aab": "cd android && gradlew bundleRelease",
    "deploy:apk": "scp -r android/app/build/outputs/apk/release/app-release.apk musiq@musiq.dergunov.net:~/web/dev/musiq.apk",
    "test": "react-scripts test",
    "test:cov": "react-scripts test --ci --coverage --watchAll=false",
    "deploy:coverage": "scp -r coverage/lcov-report/* musiq@musiq.dergunov.net:~/web/dev/coverage/web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.tsx\""
  },
  "dependencies": {
    "@amplitude/react-native": "^2.14.0",
    "@bugsnag/js": "^7.17.0",
    "@bugsnag/plugin-react": "^7.17.0",
